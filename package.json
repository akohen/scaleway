{
  "name": "scaleway",
  "version": "0.1.0",
  "description": "Barebones SDK for Scaleway APIs",
  "main": "lib/index.js",
  "scripts": {
    "start": "NODE_ENV=development babel-watch src/index.js",
    "build": "npx babel src -d lib --ignore 'src/**/*.test.js'",
    "prepublish": "npx babel src --out-dir lib",
    "test": "npx jest"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.9.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "no-console": "off"
    },
    "env": {
      "jest": true
    }
  },
  "eslintIgnore": [
    "lib/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akohen/scaleway.git"
  },
  "keywords": [
    "Scaleway"
  ],
  "author": "Alexandre KOHEN",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akohen/scaleway/issues"
  },
  "homepage": "https://github.com/akohen/scaleway#readme"
}
